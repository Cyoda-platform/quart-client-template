{
    "description": "Workflow for cat entity with typical FSM model",
    "initial_state": "none",
    "name": "cat_workflow",
    "states": {
        "feeding": {
            "transitions": {
                "finish_feeding": {
                    "action": {
                        "name": "process_finish_feeding"
                    },
                    "next": "idle"
                }
            }
        },
        "idle": {
            "transitions": {
                "start_feeding": {
                    "action": {
                        "name": "process_start_feeding"
                    },
                    "next": "feeding"
                },
                "start_sleeping": {
                    "action": {
                        "name": "process_start_sleeping"
                    },
                    "next": "sleeping"
                }
            }
        },
        "none": {
            "transitions": {
                "transition_to_idle": {
                    "next": "idle"
                }
            }
        },
        "sleeping": {
            "transitions": {
                "wake_up": {
                    "action": {
                        "name": "process_wake_up"
                    },
                    "next": "idle"
                }
            }
        }
    }
}