{
    "description": "Cat event detection workflow FSM",
    "initial_state": "none",
    "states": {
        "end": {
            "transitions": {}
        },
        "none": {
            "transitions": {
                "start_processing": {
                    "action": {
                        "name": "process_event"
                    },
                    "next": "process_event"
                }
            }
        },
        "process_event": {
            "transitions": {
                "detected_food_request": {
                    "action": {
                        "name": "send_notification"
                    },
                    "condition": {
                        "function": {
                            "name": "detected_condition"
                        },
                        "type": "function"
                    },
                    "next": "send_notification"
                },
                "no_detection": {
                    "condition": {
                        "function": {
                            "name": "not_detected_condition"
                        },
                        "type": "function"
                    },
                    "next": "end"
                }
            }
        },
        "send_notification": {
            "transitions": {
                "finish": {
                    "next": "end"
                }
            }
        }
    },
    "workflow_name": "cat_event_detection_workflow"
}