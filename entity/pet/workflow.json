{
  "description": "Purrfect Pets search workflow FSM",
  "initial_state": "none",
  "states": {
    "none": {
      "transitions": {
        "start_search": {
          "action": {
            "name": "process_pet"
          },
          "next": "search_pending"
        }
      }
    },
    "search_pending": {
      "transitions": {
        "complete_search": {
          "condition": {
            "function": {
              "name": "is_search_completed"
            },
            "type": "function"
          },
          "next": "search_completed"
        },
        "fail_search": {
          "condition": {
            "function": {
              "name": "is_search_failed"
            },
            "type": "function"
          },
          "next": "search_failed"
        }
      }
    },
    "search_completed": {
      "transitions": {}
    },
    "search_failed": {
      "transitions": {}
    }
  },
  "workflow_name": "purrfect_pets_search_workflow"
}