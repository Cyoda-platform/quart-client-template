{
    "description": "Purrfect Pets FSM Workflow",
    "initial_state": "none",
    "states": {
        "adopting": {
            "transitions": {
                "adopt_complete": {
                    "next": "done"
                }
            }
        },
        "done": {
            "transitions": {}
        },
        "fetching": {
            "transitions": {
                "fetch_complete": {
                    "next": "done"
                }
            }
        },
        "none": {
            "transitions": {
                "start_adopt": {
                    "action": {
                        "name": "process_adopt_pet"
                    },
                    "next": "adopting"
                },
                "start_fetch": {
                    "action": {
                        "name": "process_fetch_pets"
                    },
                    "next": "fetching"
                },
                "start_normalize": {
                    "action": {
                        "name": "process_normalize_pet"
                    },
                    "next": "normalizing"
                }
            }
        },
        "normalizing": {
            "transitions": {
                "normalize_complete": {
                    "next": "done"
                }
            }
        }
    },
    "version": "1.0",
    "workflow_name": "purrfect_pets_workflow"
}