{
  "description": "Pet Detail Processing FSM Workflow",
  "initial_state": "none",
  "states": {
    "none": {
      "transitions": {
        "start_processing_transition": {
          "action": {
            "name": "action_start_processing"
          },
          "next": "queued"
        }
      }
    },
    "queued": {
      "transitions": {
        "fail_transition_from_queued": {
          "action": {
            "name": "action_update_status_error"
          },
          "condition": {
            "function": {
              "name": "cond_has_error"
            },
            "type": "function"
          },
          "next": "error"
        },
        "start_background_processing": {
          "condition": {
            "function": {
              "name": "cond_is_queued"
            },
            "type": "function"
          },
          "next": "processing"
        }
      }
    },
    "error": {
      "transitions": {}
    },
    "processing": {
      "transitions": {
        "processing_failure": {
          "action": {
            "name": "action_update_status_error"
          },
          "condition": {
            "function": {
              "name": "cond_has_error"
            },
            "type": "function"
          },
          "next": "error"
        },
        "processing_success": {
          "action": {
            "name": "action_update_status_done"
          },
          "condition": {
            "function": {
              "name": "cond_is_processing"
            },
            "type": "function"
          },
          "next": "done"
        }
      }
    },
    "done": {
      "transitions": {}
    }
  },
  "workflow_name": "pet_detail_processing_workflow"
}