{
    "description": "Pet Enrichment Workflow FSM",
    "initial_state": "none",
    "states": {
        "enriched": {
            "transitions": {}
        },
        "enriching_pet": {
            "transitions": {
                "enrichment_failure_transition": {
                    "condition": {
                        "function": {
                            "name": "is_enrichment_failed"
                        },
                        "type": "function"
                    },
                    "next": "error"
                },
                "enrichment_success_transition": {
                    "condition": {
                        "function": {
                            "name": "is_enrichment_successful"
                        },
                        "type": "function"
                    },
                    "next": "enriched"
                }
            }
        },
        "error": {
            "transitions": {}
        },
        "none": {
            "transitions": {
                "start_enrichment": {
                    "action": {
                        "name": "process_pet_entity"
                    },
                    "next": "enriching_pet"
                }
            }
        }
    },
    "workflow_name": "pet_enrichment_workflow"
}