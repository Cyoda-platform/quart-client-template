{
  "description": "Purrfect Pets Search Workflow FSM",
  "initial_state": "none",
  "states": {
    "none": {
      "transitions": {
        "start_search": {
          "action": {
            "name": "process_pet_search_request"
          },
          "next": "searching_pets"
        }
      }
    },
    "searching_pets": {
      "transitions": {
        "search_failure_transition": {
          "condition": {
            "function": {
              "name": "is_search_failed"
            },
            "type": "function"
          },
          "next": "error"
        },
        "search_success_transition": {
          "condition": {
            "function": {
              "name": "is_search_successful"
            },
            "type": "function"
          },
          "next": "results_ready"
        }
      }
    },
    "error": {
      "transitions": {}
    },
    "results_ready": {
      "transitions": {}
    }
  },
  "workflow_name": "purrfect_pets_search_workflow"
}