{
    "description": "FSM constructed from user_request using typical FSM model",
    "initial_state": "none",
    "name": "user_request_workflow",
    "states": {
        "complete": {
            "transitions": {
                "finish": {
                    "action": {
                        "name": "finalize_workflow"
                    },
                    "next": "terminated"
                }
            }
        },
        "error": {
            "transitions": {
                "abort": {
                    "action": {
                        "name": "abort_workflow"
                    },
                    "next": "terminated"
                },
                "retry": {
                    "action": {
                        "name": "retry_process"
                    },
                    "next": "processing"
                }
            }
        },
        "none": {
            "transitions": {
                "start": {
                    "action": {
                        "name": "start_process"
                    },
                    "next": "processing"
                }
            }
        },
        "processing": {
            "transitions": {
                "process_complete": {
                    "action": {
                        "name": "process_data"
                    },
                    "next": "review"
                },
                "process_fail": {
                    "action": {
                        "name": "handle_error"
                    },
                    "next": "error"
                }
            }
        },
        "review": {
            "transitions": {
                "approve": {
                    "action": {
                        "name": "approve_request"
                    },
                    "next": "complete"
                },
                "reject": {
                    "action": {
                        "name": "reject_request"
                    },
                    "next": "error"
                }
            }
        },
        "terminated": {
            "transitions": {}
        }
    }
}