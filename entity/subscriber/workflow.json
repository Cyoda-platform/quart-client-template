{
    "description": "Workflow FSM for Weekly Cat Fact Subscription entity",
    "initial_state": "none",
    "states": {
        "completed": {
            "transitions": {}
        },
        "email_welcome": {
            "transitions": {
                "send_welcome_email": {
                    "action": {
                        "name": "_send_welcome_email"
                    },
                    "next": "completed"
                }
            }
        },
        "none": {
            "transitions": {
                "subscribe": {
                    "action": {
                        "name": "process_subscriber"
                    },
                    "next": "set_subscription_data"
                }
            }
        },
        "set_subscription_data": {
            "transitions": {
                "fetch_cat_fact": {
                    "action": {
                        "name": "fetch_cat_fact"
                    },
                    "next": "email_welcome"
                }
            }
        }
    },
    "version": "1.0",
    "workflow_name": "subscriber_workflow"
}