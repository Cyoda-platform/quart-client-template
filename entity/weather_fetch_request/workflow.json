{
    "description": "Weather data fetch workflow FSM",
    "initial_state": "none",
    "states": {
        "completed": {
            "transitions": {}
        },
        "failed": {
            "transitions": {}
        },
        "none": {
            "transitions": {
                "start_processing": {
                    "action": {
                        "name": "process_weather_fetch_request"
                    },
                    "next": "processing"
                }
            }
        },
        "processing": {
            "transitions": {
                "complete_failure": {
                    "condition": {
                        "function": {
                            "name": "status_is_failed"
                        },
                        "type": "function"
                    },
                    "next": "failed"
                },
                "complete_success": {
                    "action": {
                        "name": "process_data"
                    },
                    "condition": {
                        "function": {
                            "name": "status_is_completed"
                        },
                        "type": "function"
                    },
                    "next": "completed"
                }
            }
        }
    },
    "version": "1.0",
    "workflow_name": "weather_fetch_workflow"
}